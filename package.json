{
  "name": "@runnable/orion",
  "version": "1.0.0",
  "description": "Orion, the huntstman, in charge of tracking down big game, in this case big customers.",
  "main": "index.js",
  "dependencies": {
    "101": "^1.5.0",
    "bluebird": "^3.3.5",
    "intercom-client": "^2.8.0"
  },
  "devDependencies": {
    "babel-cli": "^6.7.7",
    "babel-preset-es2015": "^6.6.0",
    "code": "^2.2.1",
    "jsdoc": "^3.4.0",
    "lab": "^10.3.2",
    "proxyquire": "^1.7.4",
    "sinon": "^1.17.3",
    "standard": "^6.0.8"
  },
  "scripts": {
    "build": "babel --out-dir . lib",
    "prepublish": "not-in-install && npm run build || in-install",
